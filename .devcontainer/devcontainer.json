{
	"name": "PHP + MySQL",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"features": {
		"ghcr.io/rails/devcontainer/features/mysql-client:1": {}
	},
	"forwardPorts": [
		8080
	],
	"postCreateCommand": "composer install || true",
	"containerEnv": {
		"XDEBUG_MODE": "off"
	}
}